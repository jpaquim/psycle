
plug output out1
plug sine sine1
plug decay decay1

conn sine1 out out1 out
conn decay1 out sine1 amp

bind instrument1
	sine1 freq
	decay1 pulse
	decay1 decay

seq seq0 instrument1
	0 * * 0.0001
seq seq1 instrument1
	0 440 1 *
	1 220 1 *
	3 880 1 *
	4
seq seq2 instrument1
	0 500 1 *
	1 510 1 *
	2 500 1 *
	3 505 1 *
	4

score a
	0 seq seq0 seq1
	1 seq seq2
score main
	0 score a

