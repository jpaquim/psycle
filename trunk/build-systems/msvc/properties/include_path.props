  "$(TopSrcDir)/external-packages/luasocket-3.0";
  "$(TopSrcDir)/external-packages/boost-1.55.0/include";
  "$(TopSrcDir)/external-packages/asio-2/common";
  "$(TopSrcDir)/external-packages/asio-2/host";
  "$(TopSrcDir)/external-packages/asio-2/host/pc";
